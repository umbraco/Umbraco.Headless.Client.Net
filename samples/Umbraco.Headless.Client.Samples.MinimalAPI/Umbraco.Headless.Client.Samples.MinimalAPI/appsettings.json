{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Heartcore": {
    // The Umbraco Heartcore Project Alias
    "ProjectAlias": "demo-headless",
    // An API Key if API protection is enabled
    "ApiKey": "",
    // If Preview is used the following section is needed
    "Preview": {
      // The API Key used for the Preview API, if null the API Key from the Heartcore section is used
      "ApiKey": "",
      // TGet or set the max age for the preview cookie to be valid
      "MaxAge": "",
      // The Secret used to enter preview mode
      "Secret": ""
    },
    // If the Management api is used the following section is needed
    "Management": {
      // The API Key used for the Management API, if null the API Key from the Heartcore section is used
      "ApiKey": ""
    }
  }
}
